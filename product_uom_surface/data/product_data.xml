        <record id="product_uom_sqin" model="product.uom">
            <field name="category_id" ref="product_uom_categ_surface"/>
            <field name="name">sq in</field>
            <field name="factor" eval="1550.0031"/>
            <field name="uom_type">smaller</field>
            <field name="rounding" eval="0.01"/>
        </record>
    </data>
</odoo>
